{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "strict": false,
    "module": "amd",
    "outFile": "wwwroot/dist/main.js",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": false
  },
  "lib": [
    "ES2015"
  ],
  "include": [
    "./Scripts/**/*.ts"
  ],
  "exclude": [
    "Scripts/_Tests/**/*.spec.ts"
  ]
}
